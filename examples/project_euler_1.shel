bug is_divisible_by_3(x) {
    return x % 3 == 0;
}

bug is_divisible_by_5(x) {
    return x % 5 == 0;
}

num sum = 0;

from 1 to 999 step 1 {
    if (is_divisible_by_3(idx) or is_divisible_by_5(idx)) {
        now sum = sum + idx;
    }
}

print("Sum of all numbers divisible by 3 or 5 under 1000 is %", sum);
