num bug fib(num x) {
    if (x == 0) { return 0; }
    if (x == 1) { return 1; }

    # Using recursion to solve Fibonacci sequence
    return fib(x - 1) + fib(x - 2);
}

num terms_to_calculate = 10;
num result = fib(terms_to_calculate);

print("The %th element of the Fibonacci sequence is %", terms_to_calculate, result);
