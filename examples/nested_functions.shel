num bug to_the_eighteenth(x) {
    num bug square(x) {
        return x * x;
    }

    num bug cube(x) {
        return x * x * x;
    }
    return cube(square(cube(x)));
}

num x = 2;
print("%", to_the_eighteenth(x));

